if (center) {
      rect = Rect.fromLTRB(
        size.width / 2 + size.width / 2 * progress,
        30,
        size.width / 2,
        size.height,
      );
      //debugPrint(size.width.toString());
    } else {
      rect = Rect.fromLTRB(
        size.width / 2,
        0,
        size.width / 2 * (1 - progress),
        size.height,
      );
      //debugPrint(size.height.toString());
    }